# 3. Import and run pipeline
import sys
sys.path.append('/content/ab_newyork_etl/src')

from pipeline import ETLPipeline

# 4. Initialize pipeline
pipeline = ETLPipeline('config.yaml')

# 5. Run pipeline with Google Drive path
source_path = '/content/drive/MyDrive/Datasets/ab_newyork.zip'
processed_data = pipeline.run(source_path, 'zip')

# 6. Display results
print("First 5 rows of processed data:")
print(processed_data.head())

print("\nData Info:")
print(processed_data.info())

print("\nBasic Statistics:")
print(processed_data.describe())

# 7. Save notebook-specific outputs
import matplotlib.pyplot as plt
import seaborn as sns

# Price distribution
plt.figure(figsize=(10, 6))
sns.histplot(processed_data['price'].clip(upper=500), bins=50)
plt.title('Price Distribution (Clipped at $500)')
plt.xlabel('Price ($)')
plt.ylabel('Frequency')
plt.show()

# Room type distribution
plt.figure(figsize=(8, 6))
processed_data['room_type'].value_counts().plot(kind='bar')
plt.title('Room Type Distribution')
plt.xlabel('Room Type')
plt.ylabel('Count')
plt.xticks(rotation=45)
plt.show()

# Neighborhood group prices
plt.figure(figsize=(12, 6))
sns.boxplot(x='neighbourhood_group', y='price', 
           data=processed_data[processed_data['price'] < 500])
plt.title('Price by Neighborhood Group')
plt.xlabel('Neighborhood Group')
plt.ylabel('Price ($)')
plt.xticks(rotation=45)
plt.show()
